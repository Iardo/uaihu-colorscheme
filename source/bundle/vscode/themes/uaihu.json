    "semanticHighlighting": true,
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#FCFCFC4D",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "entity.name",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#FFC107E6"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#FFC107E6",
                "fontStyle": "normal"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "storage.type.cs"
            ],
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#FCFCFCE6",
                "fontStyle": "normal"
            }
        },
        {
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "entity.other.attribute-name.id",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.pseudo-element",
                "entity.other.attribute-name.pseudo-class"
            ],
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": [
                "entity.name.variable",
                "variable"
            ],
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#FFC107E6",
                "fontStyle": "normal"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#FFC107"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.delete"
            ],
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#FCFCFCB3",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "foreground": "#FCFCFCE6",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "foreground": "#FCFCFCE6",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#FFC107E6",
                "fontStyle": "normal"
            }
        },
        {
            "scope": [
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "string.other.link",
            "settings": {
                "foreground": "#2ECC71E6"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#FFC107E6"
            }
        },
        {
            "scope": "support.variable",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": [
                "support.type.property-name",
                "meta.object-literal.key",
                "variable.other.property.js"
            ],
            "settings": {
                "foreground": "#FFC107E6"
            }
        },
        {
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#FFC107"
            }
        },
        {
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#FCFCFCE6",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string.template meta.embedded",
            "settings": {
                "foreground": "#FCFCFC"
            }
        },
        {
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#FFC107B3",
                "fontStyle": "normal"
            }
        },
        {
            "scope": "punctuation.separator",
            "settings": {
                "foreground": "#FCFCFC"
            }
        },
        {
            "scope": [
                "punctuation.definition.template-expression",
                "punctuation.quasi.element"
            ],
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "punctuation.section.embedded",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        },
        {
            "scope": "punctuation.definition.list",
            "settings": {
                "foreground": "#FCFCFCE6"
            }
        }
    ]
}
        